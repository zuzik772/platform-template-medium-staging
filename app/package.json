{
  "name": "platform-template-frontend",
  "version": "0.0.1",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "start:dev": "next dev",
    "prebuild": "yarn env:compare && yarn sync:shared",
    "postinstall": "yarn env:compare && yarn sync:shared",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/{components,lib,pages,containers}/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/{components,lib,pages,containers}/**/*.{js,ts,tsx}\"",
    "depcheck": "depcheck",
    "gen:help": "node scripts/printHelpText.js",
    "gen:component": "node scripts/createComponent.js components",
    "gen:page": "node scripts/createNextPage.js",
    "gen:contextHook": "node scripts/createContextHook.js",
    "codegen": "mkdir -p ./src/__generated__/ && npx apollo codegen:generate --includes='src/{pages,containers,hooks,layouts}/**/*.{ts,tsx}' --localSchemaFile=../backend/schema.gql --target=typescript --excludes='**/node_modules' --outputFlat ./src/__generated__/",
    "env:compare": "node scripts/checkEnvVariables.js",
    "sync:shared": "if $(grep '^NODE_ENV' .env | grep -q 'development'); then rsync -r ../shared/ ./src/shared; fi",
    "check:code": "yarn lint",
    "init:env": "rsync -a -v --ignore-existing .env.example .env",
    "test": "yarn jest"
  },
  "dependencies": {
    "@chakra-ui/next-js": "2.2.0",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@prisma/client": "5.6.0",
    "@react-icons/all-files": "4.1.0",
    "@svgr/webpack": "8.1.0",
    "@types/react": "18.2.37",
    "@vercel/postgres": "0.5.1",
    "axios": "1.6.1",
    "bcrypt": "^5.1.1",
    "deepmerge": "4.3.1",
    "dotenv": "16.3.1",
    "formik": "2.4.5",
    "framer-motion": "10.16.5",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "next": "^14.0.3",
    "next-auth": "^5.0.0-beta.3",
    "next-i18next": "15.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.12.0",
    "react-query": "3.39.3",
    "react-svg": "16.1.30",
    "sharp": "0.32.6",
    "usehooks-ts": "2.9.1",
    "ws": "8.14.2",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-lottie": "1.2.9",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.2",
    "cypress": "^13.6.0",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "^29.7.0",
    "prettier": "3.1.0",
    "prettier-eslint": "16.1.2",
    "prisma": "5.6.0",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "react": "18.2.0"
  }
}
